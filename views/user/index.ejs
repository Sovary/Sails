<div class="container">
	<h3>Users</h3>
	<table class="table">
		<tr>
			<th>ID</th>
			<th>Name</th>
			<th>Title</th>
			<th>Email</th>
			<th>Action</th>
		</tr>
		<tbody>
		<% _.each(users,function(user){ %>
			<tr>
				<td><%= user.id %></td>
				<td><%= user.name %></td>
				<td><%= user.title %></td>
				<td><%= user.email %></td>
				<td>
					<a href="/user/show/<%= user.id%>" class="btn btn-sm btn-info">Show</a>
					<a href="/user/edit/<%= user.id%>" class="btn btn-sm btn-warning">Edit</a>
					<a href="/user/destroy/<%= user.id%>" class="btn btn-sm btn-danger">Delete</a>
				</td>
			</tr>
		<% }) %>
		</tbody>
	</table>
</div>